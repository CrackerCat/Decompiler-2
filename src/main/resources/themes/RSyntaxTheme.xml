<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RSyntaxTheme SYSTEM "theme.dtd">

<RSyntaxTheme version="1.0">

    <baseFont family="JetBrains Mono" size="15"/>
    <background color="4C566A"/>
    <caret color="c1cbc2" />
    <selection useFG="false" bg="404E51" roundedEdges="false" />
    <currentLineHighlight color="2F393C" fade="false" />
    <marginLine fg="FF0000"/>
    <markAllHighlight color="6b8189"/>
    <markOccurrencesHighlight color="5b7179" border="false" />
    <matchedBracket fg="FF0000"/>
    <hyperlinks fg="a082bd" />
    <secondaryLanguages>
        <language index="1" bg="333344" />
        <language index="2" bg="223322" />
        <language index="3" bg="332222" />
    </secondaryLanguages>

    <!-- Gutter styling. -->
    <gutterBorder color="4C566A"/>
    <lineNumbers fg="D8DEE9"/>
    <foldIndicator fg="6A8088" armedFg="a9b7c6" iconBg="2f383c" iconArmedBg="3f484c" />
    <iconRowHeader activeLineRange="3399ff" />

    <!-- Syntax tokens. -->
    <tokenStyles>
        <style token="IDENTIFIER" fg="ECEFF4" />
        <style token="RESERVED_WORD" fg="D08770" bold="true" />
        <style token="RESERVED_WORD_2" fg="D08770" bold="true" />
        <style token="ANNOTATION" fg="E8E2B7" />
        <style token="COMMENT_DOCUMENTATION" fg="6C788C" />
        <style token="COMMENT_EOL" fg="66747B" />
        <style token="COMMENT_MULTILINE" fg="66747B" />
        <style token="COMMENT_KEYWORD" fg="ae9fbf" />
        <style token="COMMENT_MARKUP" fg="ae9fbf" />
        <style token="FUNCTION" fg="EBCB8B"/>
        <style token="DATA_TYPE" fg="EBCB8B"  />
        <style token="LITERAL_BOOLEAN" fg="EBCB8B" />
        <style token="LITERAL_NUMBER_DECIMAL_INT" fg="EBCB8B" />
        <style token="LITERAL_NUMBER_FLOAT" fg="EBCB8B" />
        <style token="LITERAL_NUMBER_HEXADECIMAL" fg="EBCB8B" />
        <style token="LITERAL_STRING_DOUBLE_QUOTE" fg="A3BE8C" />
        <style token="LITERAL_CHAR" fg="EBCB8B" />
        <style token="LITERAL_BACKQUOTE" fg="EBCB8B" />
        <style token="MARKUP_TAG_DELIMITER" fg="678CB1" />
