<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RSyntaxTheme SYSTEM "theme.dtd">

<RSyntaxTheme version="1.0">

    <baseFont family="JetBrains Mono" size="15"/>
    <background color="4C566A"/>
    <caret color="FF0000"/>
    <selection fg="4C566A" bg="D08770"/>
    <currentLineHighlight color="FF0000" fade="false"/>
    <marginLine fg="FF0000"/>
    <markAllHighlight color="FF0000"/>
    <markOccurrencesHighlight color="FF0000" border="false"/>
    <matchedBracket fg="FF0000"/>
    <hyperlinks fg="FF0000"/>
    <secondaryLanguages>
        <language index="1" bg="FF0000"/>
        <language index="2" bg="FF0000"/>
        <language index="3" bg="FF0000"/>
    </secondaryLanguages>

    <gutterBorder color="4C566A"/>
    <lineNumbers fg="D8DEE9"/>
    <foldIndicator fg="FF0000" armedFg="a9b7c6" iconBg="2f383c" iconArmedBg="3f484c"/>
    <iconRowHeader activeLineRange="FF0000"/>

    <tokenStyles>
        <style token="IDENTIFIER" fg="ECEFF4"/>
        <style token="RESERVED_WORD" fg="D08770" bold="true"/>
        <style token="RESERVED_WORD_2" fg="D08770" bold="true"/>
        <style token="ANNOTATION" fg="FF0000"/>
        <style token="COMMENT_DOCUMENTATION" fg="FF0000"/>
        <style token="COMMENT_EOL" fg="FF0000"/>
        <style token="COMMENT_MULTILINE" fg="FF0000"/>
        <style token="COMMENT_KEYWORD" fg="FF0000"/>
        <style token="COMMENT_MARKUP" fg="FF0000"/>
        <style token="FUNCTION" fg="EBCB8B"/>
        <style token="DATA_TYPE" fg="EBCB8B"/>
        <style token="LITERAL_BOOLEAN" fg="EBCB8B"/>
        <style token="LITERAL_NUMBER_DECIMAL_INT" fg="EBCB8B"/>
        <style token="LITERAL_NUMBER_FLOAT" fg="EBCB8B"/>
        <style token="LITERAL_NUMBER_HEXADECIMAL" fg="EBCB8B"/>
        <style token="LITERAL_STRING_DOUBLE_QUOTE" fg="A3BE8C"/>
        <style token="LITERAL_CHAR" fg="EBCB8B"/>
        <style token="LITERAL_BACKQUOTE" fg="EBCB8B"/>
        <style token="MARKUP_TAG_DELIMITER" fg="FF0000"/>
        <style token="MARKUP_TAG_NAME" fg="FF0000" bold="true"/>
        <style token="MARKUP_TAG_ATTRIBUTE" fg="FF0000"/>
        <style token="MARKUP_TAG_ATTRIBUTE_VALUE" fg="FF0000"/>
        <style token="MARKUP_COMMENT" fg="FF0000"/>
        <style token="MARKUP_DTD" fg="FF0000"/>
        <style token="MARKUP_PROCESSING_INSTRUCTION" fg="FF0000"/>
        <style token="MARKUP_CDATA" fg="FF0000"/>
        <style token="MARKUP_CDATA_DELIMITER" fg="FF0000"/>
        <style token="MARKUP_ENTITY_REFERENCE" fg="FF0000"/>
        <style token="OPERATOR" fg="ECEFF4"/>
        <style token="PREPROCESSOR" fg="FF0000"/>
        <style token="REGEX" fg="FF0000"/>
        <style token="SEPARATOR" fg="ECEFF4"/>
        <style token="VARIABLE" fg="FF0000" bold="true"/>
        <style token="WHITESPACE" fg="FF0000"/>

        <style token="ERROR_IDENTIFIER" fg="FF0000" bg="04790e"/>
        <style token="ERROR_NUMBER_FORMAT" fg="FF0000" bg="04790e"/>
        <style token="ERROR_STRING_DOUBLE" fg="FF0000" bg="04790e"/>
        <style token="ERROR_CHAR" fg="E0E2E4" bg="FF0000"/>
    </tokenStyles>

</RSyntaxTheme>